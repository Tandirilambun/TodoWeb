@if (TempData["success"] != null)
{
    <script>
        document.addEventListener("DOMContentLoaded", function () {
            Swal.fire({
                position: "top-end",
                icon: "success",
                title: "@TempData["success"]",
                showConfirmButton: false,
                timer: 1500
            });
        })
    </script>
}