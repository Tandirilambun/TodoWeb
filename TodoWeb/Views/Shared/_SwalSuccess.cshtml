@section Scripts {
    @if (TempData["success"] != null)
    {
        <script>
            console.log("@TempData["success"]");
            Swal.fire({
                position: "top-end",
                icon: "success",
                title: "@TempData["success"]",
                showConfirmButton: false,
                timer: 1500
            });
        </script>
    }
}